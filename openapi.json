{
  "openapi": "3.0.3",
  "info": {
    "title": "Kenia API",
    "version": "1.0.0",
    "description": "Api documentation for Kenia",
    "contact": {
      "name": "KeniaAPI",
      "url": "https://github.com/Loganxav/KeniaAPI",
      "email": "sogbesansegun22@gmail.com"
    }
  },
  "servers": [
    {
      "url": "http://localhost:5500/api",
      "description": "Local server"
    }
  ],
  "paths": {
    "/auth/signup": {
      "post": {
        "description": "Sign Up New Tenant and User Record",
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultTSignUpUserResponse"
                }
              }
            }
          }
        },
        "requestBody": {
          "description": "SignUpUserRequest",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SignUpUserRequest"
              }
            }
          }
        }
      }
    },
    "/auth/signin": {
      "post": {
        "description": "Sign In An Existing User",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultTSignInResponse"
                }
              }
            }
          }
        },
        "requestBody": {
          "description": "SignInRequest",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/SignInRequest"
              }
            }
          }
        }
      }
    },
    "/auth/otp/refresh": {
      "post": {
        "description": "Refresh OTP Token",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultTRefreshOtpTokenResponse"
                }
              }
            }
          }
        },
        "requestBody": {
          "description": "RefreshOtpTokenRequest",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RefreshOtpTokenRequest"
              }
            }
          }
        }
      }
    },
    "/auth/otp/verify": {
      "post": {
        "description": "Verify OTP Token",
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResultTVerifyOtpTokenResponse"
                }
              }
            }
          }
        },
        "requestBody": {
          "description": "VerifyOtpTokenRequest",
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/VerifyOtpTokenRequest"
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "SignUpUserResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "tenantId": {
            "type": "number"
          }
        }
      },
      "ResultTSignUpUserResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "statusCode": {
            "type": "string"
          },
          "error": {
            "type": "string"
          },
          "success": {
            "type": "boolean"
          },
          "data": {
            "$ref": "#/components/schemas/SignUpUserResponse"
          }
        }
      },
      "SignUpUserRequest": {
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": [
          "firstName",
          "lastName",
          "phoneNumber",
          "email",
          "password"
        ]
      },
      "SignInResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "tenantId": {
            "type": "number"
          }
        }
      },
      "ResultTSignInResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "statusCode": {
            "type": "string"
          },
          "error": {
            "type": "string"
          },
          "success": {
            "type": "boolean"
          },
          "data": {
            "$ref": "#/components/schemas/SignInResponse"
          }
        }
      },
      "SignInRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "userType": {
            "type": "string"
          }
        },
        "required": [
          "email",
          "password",
          "userType"
        ]
      },
      "RefreshOtpTokenResponse": {
        "type": "object",
        "properties": {
          "data": {
            "type": "null"
          }
        }
      },
      "ResultTRefreshOtpTokenResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "statusCode": {
            "type": "string"
          },
          "error": {
            "type": "string"
          },
          "success": {
            "type": "boolean"
          },
          "data": {
            "$ref": "#/components/schemas/RefreshOtpTokenResponse"
          }
        }
      },
      "RefreshOtpTokenRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      },
      "VerifyOtpTokenResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "hasVerified": {
            "type": "boolean"
          },
          "isFirstTimeLogin": {
            "type": "boolean"
          },
          "lastLoginDate": {
            "type": "string",
            "format": "date"
          },
          "userType": {
            "type": "string"
          },
          "tenantId": {
            "type": "number"
          }
        }
      },
      "ResultTVerifyOtpTokenResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          },
          "statusCode": {
            "type": "string"
          },
          "error": {
            "type": "string"
          },
          "success": {
            "type": "boolean"
          },
          "data": {
            "$ref": "#/components/schemas/VerifyOtpTokenResponse"
          }
        }
      },
      "VerifyOtpTokenRequest": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "otpToken": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "otpToken"
        ]
      }
    },
    "securitySchemes": {}
  }
}