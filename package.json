{
  "name": "KeniaAPI",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,js,tsx}'",
    "dev": "ts-node-dev --respawn --transpile-only --no-notify -r tsconfig-paths/register src/index.ts",
    "test": "jest -i",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch --verbose",
    "prisma:studio": "pnpm exec prisma studio",
    "prisma:generate": "pnpm exec prisma generate --schema=src/infrastructure/internal/database/schema.prisma",
    "prisma:migrate": "pnpm exec prisma migrate dev --name migration_$(date +%s)",
    "dev:stage": "dotenv -e .env.stage -- ts-node-dev --respawn --transpile-only --no-notify -r tsconfig-paths/register src/index.ts",
    "test:stage": "dotenv -e .env.stage -- jest -i",
    "prisma:stage-generate": "dotenv -e .env.stage -- pnpm exec prisma generate --schema=src/infrastructure/internal/database/schema.prisma",
    "prisma:stage-migrate": "dotenv -e .env.stage -- pnpm exec prisma migrate dev --name migration_$(date +%s)",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "fast-glob": "^3.3.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.13",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.8.0",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.8.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/luxon": "^3.4.2",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.8",
    "prettier": "^3.3.3",
    "prisma": "^5.14.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript-eslint": "^8.0.0"
  },
  "prisma": {
    "schema": "src/infrastructure/internal/database/schema.prisma"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm run lint && pnpm run format"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "pnpm run lint:fix",
      "pnpm run format"
    ]
  }
}
