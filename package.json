    "ts-node": "^10.9.2",
    "tsyringe": "^4.8.0",
    "typescript": "^5.4.5",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
