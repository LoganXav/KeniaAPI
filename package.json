    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "fast-glob": "^3.3.2",
    "helmet": "^7.1.0",
