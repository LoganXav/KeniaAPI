{
  "name": "KeniaAPI",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "dev": "dotenv -e .env.dev -- ts-node-dev --respawn --transpile-only --no-notify -r tsconfig-paths/register src/index.ts",
    "test": "dotenv -e .env.dev -- jest -i",
    "test:coverage": "dotenv -e .env.dev -- jest --coverage",
    "test:watch": "dotenv -e .env.dev -- jest --watch --verbose",
    "prisma:generate": "dotenv -e .env.dev -- pnpm exec prisma generate --schema=src/infrastructure/internal/database/schema.prisma",
    "prisma:migrate": "dotenv -e .env.dev -- pnpm exec prisma migrate dev --name migration_$(date +%s)",
    "prisma:studio": "dotenv -e .env.dev -- pnpm exec prisma studio",
    "prisma:prod-generate": "pnpm exec prisma generate --schema=src/infrastructure/internal/database/schema.prisma",
    "prisma:prod-migrate": "pnpm exec prisma migrate dev --name migration_$(date +%s)"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
